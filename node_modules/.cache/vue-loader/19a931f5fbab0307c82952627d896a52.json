ript>\nexport default {\n    name: \"Patients\",\n    props: {\n        items: {\n            type: Array\n        }\n    },\n    methods: {\n        setPatientAllocation(id, first_name, last_name){\n            console.log(\"name:\" + first_name)\n            this.$store.commit('allocatePatient', {\n                id:id, \n                first_name: first_name,\n                last_name: last_name,                \n            });\n            this.$router.push({path:\"/allocations/add\"});\n        }\n    }\n}\n</script>"]}]}